function calcFuel(mass) {
if(mass <= 6 )
    return 0;
  mass /= 3; 
  mass = Math.floor(mass);
  mass -= 2;
  return mass
};

function recClac(mass) {
  var temp = mass
  if(mass <= 6)
  	return mass;
  temp /= 3; 
  temp = Math.floor(temp);
  temp -= 2;
  return mass + recClac(temp);
};

